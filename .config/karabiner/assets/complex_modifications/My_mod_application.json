{
	"title": "Launch apps",
	"rules": [
		{
			"description": "Launch apps F13 -> Activity Monitor",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "f13",
						"modifiers": {
							"optional": [
								"any"
							]
						}
					},
					"to": [
						{
							"shell_command": "open '/System/Applications/Utilities/Activity Monitor.app'"
						}
					]
				}
			]
		},
